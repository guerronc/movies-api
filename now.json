{
    "name": "platzivideo",
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes":[
        {
            "src": "(/(.*))",
            "dest": "/index.js"
        }
    ],
    "env":{
        "DB_USER":"@platzivideo-db-user",
        "DB_PASSWORD": "@platzivideo-db-password",
        "DB_HOST1": "@platzivideo-db-host1",
        "DB_HOST2": "@platzivideo-db-host2",
        "DB_HOST3": "@platzivideo-db-host3",
        "DB_NAME": "@platzivideo-db-name",
        "DB_PORT": "@platzivideo-db-port"
    }
}